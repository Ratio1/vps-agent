# Required: Hostinger Personal Access Token from hPanel
HOSTINGER_API_TOKEN=replace-with-hostinger-api-token

# Optional: extra logs from hostinger-api-mcp
HOSTINGER_MCP_DEBUG=false

# Optional: enable local ambient AWS/GCP/Azure CLI session checks in regression runner
REGRESSION_USE_LOCAL_SESSIONS=false

# Optional: AWS credentials/auth hints for regression checks
AWS_PROFILE=
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_SESSION_TOKEN=
AWS_REGION=

# Optional: GCP credentials/auth hints for regression checks
GOOGLE_APPLICATION_CREDENTIALS=
GOOGLE_CLOUD_PROJECT=
GOOGLE_CLOUD_REGION=

# Optional: Azure credentials/auth hints for regression checks
AZURE_TENANT_ID=
AZURE_CLIENT_ID=
AZURE_CLIENT_SECRET=
AZURE_SUBSCRIPTION_ID=

# Optional: OVH credentials/auth hints for regression checks
OVH_APPLICATION_KEY=
OVH_APPLICATION_SECRET=
OVH_CONSUMER_KEY=
OVH_ENDPOINT=ovh-eu

# Optional: only needed if you use API-key auth for Codex CLI instead of account login
OPENAI_API_KEY=
